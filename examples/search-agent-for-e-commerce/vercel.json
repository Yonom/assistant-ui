{
  "version": 2,
  "builds": [
    {
      "src": "src/app/api/chat/validate.ts",
      "use": "@vercel/node"
    },
    {
      "src": "next.config.mjs",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/validate",
      "dest": "src/app/api/chat/validate.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
